<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE package SYSTEM "http://pear.php.net/dtd/package-1.0">
<package version="1.0">
  <name>Math_RPN</name>
  <summary>Reverse Polish Notation.</summary>
  <description>Change Expression To RPN (Reverse Polish Notation) and evaluate it.</description>
  <maintainers>
    <maintainer>
      <user>mszczytowski</user>
      <name>Maciej Szczytowski</name>
      <email>admin@e-rower.pl</email>
      <role>lead</role>
    </maintainer>
  </maintainers>
  <release>
    <version>1.1.1</version>
    <date>2005-01-03</date>
    <license>PHP License</license>
    <state>stable</state>
    <notes>This is a bug fix release. It fixes a wrong parsing difference operator after right bracket.</notes>
    <provides type="class" name="Math_Rpn" />
    <provides type="function" name="Math_Rpn::getOperators" />
    <provides type="function" name="Math_Rpn::addOperator" />
    <provides type="function" name="Math_Rpn::calculate" />
    <provides type="function" name="Math_Rpn::evaluate" />
    <provides type="function" name="Math_Rpn::getInputArray" />
    <provides type="function" name="Math_Rpn::getRpnArray" />
    <provides type="function" name="Math_Rpn::getTimer" />
    <filelist>
      <file role="php" baseinstalldir="Math" name="RPN.php"/>
    </filelist>
  </release>
  <changelog>
    <release>
      <version>1.1</version>
      <date>2004-07-12</date>
      <state>stable</state>
      <notes>This is a bug fix release. It fixes a wrong assumption of unary minus if it's preceded with space.
</notes>
    </release>
    <release>
      <version>1.0</version>
      <date>2003-07-30</date>
      <state>stable</state>
      <notes>This is the first release.
</notes>
    </release>
  </changelog>
</package>
